 <% if flash.now[:messages]%>
    <% flash.now[:messages].each do |message| %>
    <h4 style="color: white;"> <%= message %> </h4>
    <%end%>
<%end%>

<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<h6 style="color: white; text-align: right"><a style="color: white" href="/sessions/new">Login</a>


    <div class="home-video-wrapper">
        <%= video_tag "login_video.mp4", autoplay: :autoplay, loop: :loop, mute: :mute, class: "home_video"%>
            <div class= "form_div" >
                 <%= form_for @user do |f| %>
                    <h6 style="color: white"> Name <%= f.text_field :name %> </h6>
                    <h6 style="color: white"> Password  <%= f.password_field :password%> </h6>
                    <%= f.submit "Create New User" %>
                <%end%>
            </div>
    </div>




